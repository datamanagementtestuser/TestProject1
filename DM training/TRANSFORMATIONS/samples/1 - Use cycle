var populationbysex = Knoema.UseDataset("populationBySex");
populationbysex.LoadData();
foreach (var country in populationbysex.Dimensions["Country"])
{
	if (populationbysex[country, "SP.POP.TOTL.FE.IN", Frequency.Annual, "Value"] > populationbysex[country, "SP.POP.TOTL.MA.IN", Frequency.Annual, "Value"])
	{
		WriteLine("W > M for " + country["Name"]);
	}
	else
	{
		WriteLine("M > F for " + country["Name"]);
	}
}
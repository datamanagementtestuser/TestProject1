var populationbysex = Knoema.UseDataset("populationBySex");
populationbysex.LoadData();
foreach (var country in populationbysex.Dimensions["Country"])
{
	populationbysex[country, "SP.POP.TOTL", Frequency.Annual, "Value"] = populationbysex[country, "SP.POP.TOTL.FE.IN", Frequency.Annual, "Value"]+populationbysex[country, "SP.POP.TOTL.MA.IN", Frequency.Annual, "Value"];
	populationbysex[country, "KN.B1", "Units"] = "Persons";
}
populationbysex.SaveData(SaveOptions.Update);
var populationbysex = Knoema.UseDataset("populationBySex");
populationbysex.LoadData(populationbysex.Dimensions["Attributes"]["Value"]);
foreach (var series in populationbysex)
{
	if (!populationbysex[series.Coordinate].IsEmpty)
    {
        var value = populationbysex[series.Coordinate][new DateTime(2019, 1, 1)];
        if (series.Coordinate["Attributes"] == "Value" && value != null)
        {
            if ((double)value > 100000000)
            {
                WriteLine(series.Coordinate["Country"]["Name"] + " - " + series.Coordinate["Series"]["Name"] + ": " + value);
            }
        }
    }
}
WriteLine("Thats all");;
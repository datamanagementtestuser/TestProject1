var popbysex = Knoema.UseDataset("popBySex");
popbysex.LoadData();
foreach (var series in popbysex)
{
	if (!popbysex[series.Coordinate].IsEmpty)
	{
		if (series.Coordinate["Attributes"] == "Value")
		{
			var value2000 = popbysex[series.Coordinate][new DateTime(2000, 1, 1)];
            if (value2000 != null)
			{
				if ((double)value2000 >20000000)
				{
					WriteLine(series.Coordinate["Country"]["Name"]  + " - " + series.Coordinate["Series"]["Name"] + ":  " + value2000);
				}
			}
		}
	}
}